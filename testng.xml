<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Reporter">

	<groups>
		<run>
			<include name="sanity"></include>
			<include name="smoke"></include>
		</run>
	</groups>

	<listeners>
		<listener class-name="SeleniumPratices.ListnerImpl" /> <!-- Replace
			with your listener class -->
	</listeners>
	<test thread-count="5" name="Test" parallel="false" verbose="2">
		<packages>
<!--		  <package name="testcases.*"></package>-->
		  <package name="SeleniumPratices.*"></package>
		  <package name="pages.*"></package>
		</packages>
			
	</test> <!--
	Test -->
</suite> <!--
Suite -->
